{
    "extends": "tslint:recommended",
    "rules": {
        "quotemark": false,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning"
        },
        "no-unused-variable": {
            "severity": "warning"
        },
        "object-literal-sort-keys": false,
        "prefer-const": {
            "severity": "warning"
        },
        "no-empty": false,
        "no-trailing-whitespace": false,
        "member-ordering": [true, {
            "order": [
                "static-field",
                "instance-field",
                "constructor",
                "static-method",
                "instance-method"
            ]
        }]
    }
}