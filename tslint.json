{
    "extends": "tslint:recommended",
    "rules": {
        "quotemark": false,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning"
        },
        "no-unused-variable": {
            "severity": "warning"
        },
        "object-literal-sort-keys": false,
        "prefer-const": {
            "severity": "warning"
        },
        "ordered-imports": {
            "severity": "warning"
        },
        "max-classes-per-file": false,
        "no-empty": false,
        "no-trailing-whitespace": false,
        "member-ordering": [true, {
            "order": [
                "static-field",
                "instance-field",
                "constructor",
                "static-method",
                "instance-method"
            ]
        }],
        "arrow-parens": false,
        "no-empty-interface": false,
        "no-namespace": false,
        "curly": false,
        "max-line-length": false,
        "no-var-requires": false,
        "object-literal-shorthand": false,
        "no-bitwise": false
    }
}