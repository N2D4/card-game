{
  "name": "card-game",
  "version": "1.0.0",
  "description": "## hello",
  "client_js_src": "src/client/index.ts",
  "client_js_bundle": "dist/index.js",
  "client_files": {
    "index.html": "src/client/index.html",
    "index.css": "src/client/index.css",
    "index.js": "dist/index.js",
    "assets/": "assets/"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "dev-bundle": "webpack --mode=development",
    "prod-bundle": "webpack --mode=production",
    "run": "node lib/server/main.js",
    "start": "npm run-script build:js && npm run-script dev-bundle && npm run-script run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/N2D4/card-game.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/N2D4/card-game/issues"
  },
  "homepage": "https://github.com/N2D4/card-game#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-typescript": "^7.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/socket.io": "^2.1.0",
    "@types/socket.io-client": "^1.4.32",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.4",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  }
}
