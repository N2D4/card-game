{
    "presets": [
        "@babel/env",
        "@babel/typescript"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        ["module-resolver", {
            "root": ["./"],
            "alias": {
                "src": "./src",
                "client": "./src/client",
                "common": "./src/common",
                "server": "./src/server",
                "package.json": "./package.json"
            }
        }]
    ]
}